p1 <- ggplot(data = temp, aes(x = Metadata_Geno, y = Intensity_MeanIntensity_AQP5)) +
geom_violin(fill = "#c90016", alpha = .5) + theme_classic()
return(p1)
}
cellprofiler_quickpeek("~/Desktop/JEVtemp.csv")
cellprofiler_quickpeek("~/Desktop/JEVtemp2.csv")
ggplot(data = aqp5staining, aes(x = Metadata_Mouse, y = AreaShape_Area)) +
geom_violin(fill = "#c90016", alpha = .5) + theme_classic()
cellprofiler_read <- function(cellprofiler_filename, area_min = 0, area_max = Inf){
temp <- read.csv(cellprofiler_filename)
temp$Metadata_Mouse <- as.factor(temp$Metadata_Mouse)
temp$Metadata_Geno <- as.factor(temp$Metadata_Geno)
temp$Metadata_Week <- as.factor(temp$Metadata_Week)
temp <- filter(temp, AreaShape_Area > area_min)
temp <- filter(temp, AreaShape_Area < area_max)
return(temp)
}
aqp5staining <- cellprofiler_read("~/Desktop/JEVtemp.csv")
aqp5staining <- cellprofiler_read("~/Desktop/JEVtemp.csv", area_min = 100, area_max = 1500)
ggplot(data = aqp5staining, aes(x = Metadata_Mouse, y = AreaShape_Area)) +
geom_violin(fill = "#c90016", alpha = .5) + theme_classic()
