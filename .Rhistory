countA <- function (seq) {
# This function finds the number of 'A's
# in a given nucleotide sequence
# Convert input sequence to lower case character vector
seq_lower <- strsplit(tolower(seq),"")[[1]]
# Count the number of As
a_count <- length(which(seq_lower=="a"))
# Returning the A count
return(a_count)
}
